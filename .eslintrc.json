{
    "plugins": [
        "cypress",
        "chai-friendly"
      ],
    "env": {
        "es6": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "extends": [
            "plugin:cypress/recommended"
          ],
        "no-unused-expressions": 0,
        "chai-friendly/no-unused-expressions": 2
    }
}